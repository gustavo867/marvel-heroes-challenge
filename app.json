{
  "expo": {
    "name": "marvel-heroes-challenge",
    "slug": "marvel-heroes-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/marvel.png",
    "splash": {
      "image": "./src/assets/chars/spider-man.png",
      "resizeMode": "cover",
      "backgroundColor": "rgba(0, 0, 0, 0.85)"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
